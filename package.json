{
  "name": "yunnan-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --port=4203 --host=0.0.0.0",
    "build": "ng build",
    "precommit": "npm run lint-staged",
    "lint-staged": "lint-staged",
    "lint:ts": "tslint -p src/tsconfig.app.json -c tslint.json 'src/**/*.ts'",
    "lint:style": "stylelint \"{src}/**/*.scss\" --syntax scss",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p ng-package.json"
  },
  "private": false,
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.6",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "husky": "^3.0.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^5.5.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  },
  "dependencies": {
    "@angular/animations": "^7.1.4",
    "@angular/cdk": "^7.2.1",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/material": "^7.2.1",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "@antv/g2": "^3.4.9",
    "angular-split": "^3.0.2",
    "core-js": "^2.5.4",
    "echarts": "^4.2.1",
    "editorconfig-tools": "^0.1.1",
    "hammerjs": "^2.0.8",
    "highlight.js": "^9.15.9",
    "lint-staged": "^9.2.4",
    "markdown-it": "^9.1.0",
    "ng-zorro-antd": "^7.0.0-rc.3",
    "ngx-markdown": "^8.1.0",
    "prismjs": "^1.17.1",
    "rxjs": "^6.5.2",
    "tslib": "^1.9.0",
    "yunnan-ui": "0.0.0",
    "zone.js": "~0.8.26"
  },
  "lint-staged": {
    "*.{cmd,html,json,md,sh,txt,xml,yml}": [
      "editorconfig-tools fix",
      "git add"
    ],
    "*.ts": [
      "npm run lint:ts",
      "prettier --write",
      "git add"
    ],
    "src/**/*.scss": [
      "npm run lint:style",
      "prettier --write",
      "git add"
    ]
  }
}
